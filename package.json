		"@stripe/stripe-js": "^4.10.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/d3-array": "^3.2.2",
		"@types/d3-time": "^3.0.4",
		"@types/mapbox__mapbox-gl-geocoder": "^5.0.0",
